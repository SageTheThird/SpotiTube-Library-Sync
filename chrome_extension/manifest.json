{
  "manifest_version": 3,
  "name": "SpotiTube-Library-Sync",
  "version": "1.0",
  "permissions": ["activeTab"],
  "host_permissions": ["http://localhost:5000/"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
